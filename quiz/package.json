{
  "name": "quiz",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "watch": "nodemon --transpile-only app.ts",
    "build": "yarn generate && tsc && cp -r public views graphql favicon package.json prisma yarn.lock dist/ && find dist -name '*.ts' | grep -v context.ts | xargs rm",
    "valid": "npx depngn 16",
    "prisma:migration": "prisma migrate deploy && prisma generate",
    "schema": "echo -e $(get-graphql-schema http://localhost:3000/api --json) > schema.json",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:nexus": "ts-node --transpile-only ./graphql/nexus",
    "generate:prisma": "prisma generate"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.0",
    "@prisma/client": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.21.3",
    "marked": "^4.2.12",
    "morgan": "^1.10.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.5",
    "nodemailer": "^6.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/marked": "^4.0.8",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.2",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "get-graphql-schema": "^2.1.2",
    "nodemon": "^2.0.22",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
